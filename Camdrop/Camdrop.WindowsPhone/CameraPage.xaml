<Page
    x:Class="Camdrop.CameraPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Camdrop"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="#FF000000"
    DataContext="{Binding RelativeSource={RelativeSource Self}}">

    <StackPanel x:Name="LayoutRoot" Margin="0,-32,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Center">

        <Image x:Name="imgCameraFeed" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="0,0,0,0" />
        
    </StackPanel>
</Page>
